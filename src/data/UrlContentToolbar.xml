<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE MEEGO_IM_TOOLBAR_WIDGET SYSTEM 'ImToolbarDTD.dtd'>
<input-method version="1">
  <toolbar>
    <items>
      <button name="/" text="/" alignment="center" priority="120">
        <actions>
          <sendstring string="/"/>
        </actions>
      </button>
      <button name="_domain" text=".com" alignment="center" priority="119">
        <actions>
          <sendstring string=".com"/>
        </actions>
      </button>
    </items>
    <layout>
      <item name="/" />
      <item name="_domain" />
    </layout>
  </toolbar>
</input-method>
